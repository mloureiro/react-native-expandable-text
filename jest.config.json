{
  "preset": "react-native",
  "transform": {
    "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
    "^.+\\.tsx?$": "ts-jest"
  },
  "testMatch":  ["<rootDir>/src/**/*.(spec|test).(ts|tsx|js|jsx)"],
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "modulePaths": ["<rootDir>"],
  "setupFiles": ["<rootDir>/jest.setup.js"]
}
